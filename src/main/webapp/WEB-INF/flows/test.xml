<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd" start-state="init">
        
        <view-state id="init" view="init">
                <transition on="ok1" to="medium"></transition>
        </view-state>
        
        <view-state id="medium" view="medium">
                <transition on="ok2" to="final"></transition>
        </view-state>
        
        <end-state id="final" view="final"/>
        
</flow>